{
  "name": "footer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve footer-web-element --disable-host-check --port 4204",
    "build": "ng build footer-web-element --output-hashing none && npm run concat",
    "watch": "npm run build --watch --configuration development",
    "test": "ng test",
    "fakedeploy": "npm run build && node fake-deploy.js",
    "concat": "concat-glob-cli -f dist/footer-web-element/*.js -o dist/footer-web-element/footer.js",
    "cpy": "cpy package-build.json dist/footer-web-element --rename=package.json",
    "publish:footer": "npm run build && npm run concat && node upgrade-package-version.js && npm run cpy && node publish-package.js"
  },
  "dependencies": {
    "@angular/animations": "^16.1.1",
    "@angular/common": "^16.1.1",
    "@angular/compiler": "^16.1.1",
    "@angular/core": "^16.1.1",
    "@angular/elements": "^16.1.1",
    "@angular/forms": "^16.1.1",
    "@angular/platform-browser": "^16.1.1",
    "@angular/platform-browser-dynamic": "^16.1.1",
    "@angular/router": "^16.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/cli": "^16.1.0",
    "@angular/compiler-cli": "^16.1.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concat-glob-cli": "^0.1.0",
    "cpy-cli": "^4.1.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^16.1.0",
    "typescript": "~4.9.5"
  }
}
